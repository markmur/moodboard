{
  "name": "moodboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "firebase": "^5.1.0",
    "firebaseui": "^3.1.1",
    "grid-styled": "^4.2.0",
    "memoize-one": "^4.0.0",
    "ow": "^0.6.0",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-draggable": "^3.0.5",
    "react-dropzone": "^4.2.12",
    "react-icons": "^2.2.7",
    "react-input-autosize": "^2.2.1",
    "react-resizable": "^1.7.5",
    "react-rnd": "^8.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-last-location": "^1.1.0",
    "react-scripts": "1.1.4",
    "react-switch": "^3.0.4",
    "react-tippy": "^1.2.2",
    "serve": "^9.1.0",
    "styled-components": "^3.3.3",
    "styled-system": "^2.3.2"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "deploy": "yarn build && firebase deploy"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add",
      "xo"
    ]
  },
  "precommit": {
    "run": [
      "lint-staged"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "browser"
    ],
    "prettier": true,
    "space": true,
    "rules": {
      "no-warning-comments": 0,
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": 0,
      "no-alert": 0,
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    },
    "extends": "xo-react",
    "plugins": [
      "react"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "clean-tag": "^2.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.23.0",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.10.0",
    "firebase-tools": "^3.19.1",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.6",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.0.0",
    "xo": "^0.21.1"
  }
}
